  <field name="name">spot.tv.com.pany.view.form</field>
  <field name="model">spot.tv.company</field>
  <field name="arch" type="xml">
   <form>
    <group>
     <field name="name" />
     <field name="media_seller" />
     <field name="target_audience" />
     <field name="country" />
    </group>
    <group>
     <field name="prime_time_begin" widget="float_time" />
     <field name="prime_time_end" widget="float_time" />
     <field name="vat" />
     <field name="own_media" />
     <field name="prime_time_factor" />
    </group>
    <field name="description" />
   </form>
  </field>
 </record>
