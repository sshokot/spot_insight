 <record id="map_adv_brand_procat_view_tree" model="ir.ui.view">
  <field name="name">spot.map.adv.brand.procat.view.tree</field>
  <field name="model">spot.map.adv.brand.procat</field>
  <field name="arch" type="xml">
   <tree>
    <field name="advertiser_raw_data" />
    <field name="advertiser" />
    <field name="brand_raw_data" />
    <field name="brand" />
    <field name="product_category_raw_data" />
    <field name="product_category" />
    <field name="is_zero_budget" />
   </tree>
  </field>
 </record>
 <record id="map_adv_brand_procat_view_form" model="ir.ui.view">
  <field name="name">spot.map.adv.brand.procat.view.form</field>
  <field name="model">spot.map.adv.brand.procat</field>
  <field name="arch" type="xml">
   <form>
    <group>
     <group>
      <field name="advertiser_raw_data" />
      <field name="advertiser" />
     </group>
     <group>
      <field name="brand_raw_data" />
      <field name="brand" />
     </group>
     <group>
      <field name="product_category_raw_data" />
      <field name="product_category" />
     </group>
    </group>
   </form>
  </field>
 </record>

